{
  "icons": {
    "16":"icon.png",
    "32":"icon.png",
    "48":"icon.png",
    "128":"icon.png"
  },
  "manifest_version": 2,
  "name": "pin cracker A666",
  "description": "",
  "version": "1",

  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": [
        "https://*.google.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],


  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_css": "sample.css"
  }
}